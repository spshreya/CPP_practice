#include <iostream>
#include<cstdio>
#include<cmath>
using namespace std;

void update(int *a,int *b) {
       int n,m;
        n= *a+*b;
        m= abs(*a-*b);
        cout<<n<<endl<<m;
}

int main() {
    int a, b;
    int *pa = &a, *pb = &b;
    
    cin>>a>>b;
    update(pa, pb);

    return 0;
}
