#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int q,n,a,b,k;
    cin>>q>>n;

    vector<vector<int>> vec(n);

for(int i=0;i<n;i++)

{

cin >> k;

vec[i]=vector<int>(k);

for(int j=0;j<k;j++)

{

cin >> vec[i][j];

}

}
    for(int i=0;i<q;i++){
        cin>>a>>b;
        cout<<vec[a][b]<<endl;
    }

    return 0;
}
